<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal" >

    <TextView
        android:id="@+id/adminlecturerlist_index"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:padding="6dp"
        android:text="1" />

    <TextView
        android:id="@+id/adminlecturerlist_branch"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:padding="6dp"
        android:text="Electronics and Telecommunication" />

    <TextView
        android:id="@+id/adminlecturerlist_year"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:padding="6dp"
        android:text="Electronics and Telecommunication" />

    <TextView
        android:id="@+id/adminlecturerlist_subject"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:padding="6dp"
        android:text="Electronics and Telecommunication" />

</LinearLayout>